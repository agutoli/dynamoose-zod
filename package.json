{
  "name": "dynamoose-zod",
  "version": "0.1.3",
  "description": "Convert Zod schemas to Dynamoose schema definitions easily",
  "keywords": [
    "zod",
    "dynamoose",
    "dynamodb",
    "schema",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:agutoli/dynamoose-zod.git"
  },
  "homepage": "https://github.com/agutoli/dynamoose-zod",
  "bugs": {
    "url": "https://github.com/agutoli/dynamoose-zod/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Bruno Agutoli <bruno.agutoli@gmail.com>",
  "scripts": {
    "build": "tsup",
    "test": "jest"
  },
  "peerDependencies": {
    "dynamoose": "^4.0.4",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "dynamoose": "^4.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "zod": "^3.22.0"
  },
  "license": "MIT"
}
